<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="xyz.tonalall.impl.mapper.Db2.UserMapper">
    <select id="un" resultType="xyz.tonalall.impl.entity.User">
        select *
        from user
        where #{username} = username
    </select>

    <select id="findFlow" resultType="java.util.Map">
        select * from `3-15` where time = '12:0'
    </select>
</mapper>